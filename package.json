{
  "name": "prettyjson-256",
  "version": "1.1.5",
  "description": "JSON Pretty Print in 256 colors with extras",
  "main": "dist/index.babel.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build": "babel -d ./dist ./lib -s",
    "lint": "esw lib test",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch",
    "start": "node ./dist/index.js",
    "test": "mocha",
    "test:mochista": "mochista --sourceFilesExclude babel.js --reporter spec --opts ./test/mocha.opts",
    "test:watch": "mocha --watch --reporter spec",
    "integration": "nodemon test/integration.babel.js",
    "coverage": "better-npm-run coverage",
    "coverage:watch": "npm run test:mochista -- --watch"
  },
  "betterScripts": {
    "coverage": {
      "command": "./node_modules/.bin/istanbul cover --dir ./coverage --report text --report html node_modules/.bin/_mocha -- ./test/**/*.js --opts ./test/mocha.opts --reporter spec",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "keywords": [
    "json",
    "prettyjson",
    "256",
    "printjson",
    "inspect"
  ],
  "bin": {
    "prettyjson256": "./bin/prettyjson256"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/njhoffman/prettyjson-256.git"
  },
  "author": "Nick Hoffman",
  "license": "ISC",
  "dependencies": {
    "ansi-256-colors": "^1.1.0",
    "fs": "0.0.1-security",
    "lodash": "^4.8.2",
    "minimist": "^1.2.0",
    "path": "^0.12.7",
    "vm": "^0.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "better-npm-run": "0.0.15",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^3.1.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.2.0",
    "mochista": "^0.10.2",
    "proxyquire": "^1.7.11",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "watchify": "^3.9.0",
    "yargs": "^7.1.0"
  }
}
