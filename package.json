{
  "name": "prettyjson-256",
  "version": "1.5.18",
  "description": "JSON Pretty Print in 256 colors with extras",
  "main": "dist/prettyjson.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build": "babel -d ./dist ./lib -s",
    "lint": "esw lib test --color",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha",
    "test:mochista": "mochista --opts ./test/mochista.opts --reporter spec",
    "test:watch": "mocha --watch --reporter spec",
    "coverage": "better-npm-run coverage",
    "coverage:watch": "npm run test:mochista -- --watch",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "changelog": "git log $(git describe --tags --abbrev=0)..HEAD --pretty='* %s' --first-parent"
  },
  "betterScripts": {
    "coverage": {
      "command": "./node_modules/.bin/istanbul cover --dir ./coverage --report text --report lcov node_modules/.bin/_mocha -- --opts ./test/mocha.opts --reporter spec",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "keywords": [
    "json",
    "prettyjson",
    "256",
    "formatting",
    "colors",
    "printjson",
    "inspect"
  ],
  "bin": {
    "prettyjson256": "./bin/prettyjson256"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/njhoffman/prettyjson-256.git"
  },
  "bugs": {
    "url": "https://github.com/njhoffman/prettyjson-256/issues"
  },
  "author": "Nick Hoffman",
  "license": "MIT",
  "dependencies": {
    "ansi-256-colors": "^1.1.0",
    "babel-cli": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "fs": "0.0.1-security",
    "lodash": "^4.8.2",
    "minimist": "^1.2.0",
    "path": "^0.12.7",
    "vm": "^0.1.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "better-npm-run": "0.0.15",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coveralls": "^2.11.15",
    "eslint": "6.6.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-watch": "6.0.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mochista": "^0.11.0",
    "prettier": "^1.19.1",
    "proxyquire": "^1.7.11",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "watchify": "^3.9.0",
    "yargs": "^7.1.0"
  }
}
